<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<!-- SQL문 정의 시 VO 간단히 표현 -->
	<typeAliases>
    	<typeAlias type="com.modifyk.accountbook.member.MemberVO" alias="memberVO"></typeAlias>
      	<typeAlias type="com.modifyk.accountbook.asset.AssetVO" alias="assetVO"></typeAlias>
   	   	<typeAlias type="com.modifyk.accountbook.category.CategoryVO" alias="categoryVO"></typeAlias>
   	   	<typeAlias type="com.modifyk.accountbook.account.AccountVO" alias="accountVO"></typeAlias>
   	   	<typeAlias type="com.modifyk.accountbook.aim.AimVO" alias="aimVO"></typeAlias>
   	   	<typeAlias type="com.modifyk.accountbook.aim.AimJoinVO" alias="aimjoinVO"></typeAlias>
   	   	<typeAlias type="com.modifyk.accountbook.aim.AimTotalVO" alias="aimtotalVO"></typeAlias>
   	   	<typeAlias type="com.modifyk.accountbook.transfer.TransferVO" alias="transferVO"></typeAlias>
   	   	<typeAlias type="com.modifyk.accountbook.repeat.RepeatVO" alias="repeatVO"></typeAlias>
	</typeAliases>
	
   	<!-- SQL문 정의하는 파일들의 목록 -->
   	<mappers>
    <mapper resource="mapper/memberMapper.xml" />
    	<mapper resource="mapper/assetMapper.xml" />
      	<mapper resource="mapper/categoryMapper.xml" />
     	<mapper resource="mapper/accountMapper.xml" />
      	<mapper resource="mapper/aimMapper.xml" />
      	<mapper resource="mapper/aimtotalMapper.xml" />
      	<mapper resource="mapper/transferMapper.xml" />
      	<mapper resource="mapper/repeatMapper.xml" />
   	</mappers>
</configuration>

